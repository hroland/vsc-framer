  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#83848A"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "Entity",
      "scope": "entity",
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    // Typescript
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    // Typescript
    {
      "name": "Keywords",
      "scope": ["keyword.control.conditional", "keyword.control.import"],
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#83848A"
      }
    },
    {
